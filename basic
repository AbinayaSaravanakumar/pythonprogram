// Python program to check if the number is an Armstrong number or not
 
num = int(input("Enter a number: "))
sum = 0 
temp = num
while temp > 0:
   digit = temp % 10
   sum += digit ** 3
   temp //= 10
if num == sum:
   print(num,"is an Armstrong number")
else:
   print(num,"is not an Armstrong number")
//python program for odd or even

num = int(input("Enter a number: "))
if (num % 2) == 0:
   print("{0} is Even".format(num))
else:
   print("{0} is Odd".format(num))

//python program to find palindrome

s = "malayalam"   

i,j = 0, len(s) - 1   

is_palindrome = True  
while i < j:
    if s[i] != s[j]:   
        is_palindrome = False
        break
    i += 1
    j -= 1

if is_palindrome:
    print("Yes") 
else:
    print("No") 

//prime or not

from sympy import *

 
geek1 = isprime(30)
geek2 = isprime(13)
geek3 = isprime(2)

print(geek1) 
print(geek2) 
print(geek3) 
